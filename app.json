{
    "name": "Travel Penguin Bot",
    "description": "LINE Bot of Traveling Penguin",
    "repository": "https://github.com/icoxfog417/TravelPenguinBot",
    "keywords": ["Python", "tornado", "LINE"],
    "addons": [
        "fixie:tricycle"
    ],
    "env": {
        "SECRET_TOKEN": {
            "description": "A secret key for verifying the integrity of signed cookies.",
            "generator": "secret"
        },
        "LINE_CHANNEL_ID": {
            "description": "A line channel id."
        },
        "LINE_CHANNEL_SECRET": {
            "description": "A linse secret key."
        },
        "LINE_MID": {
            "description": "A line mid."
        },
        "GOOGLE_API_KEY": {
          "description": "A key for Google Place API."
        }
    }
}
